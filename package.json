{
  "name": "lethean-dvpn",
  "version": "0.1.2",
  "description": "Lethean VPN Node",
  "main": "main.js",
  "scripts": {
    "start": "$HOME/.deno/bin/deno run --allow-net --allow-env --allow-run --allow-read --allow-write --unstable src/server.ts",
    "backend": "$HOME/.deno/bin/deno run --allow-net --allow-env --allow-run --allow-read --allow-write --unstable src/server.ts backend start --home-dir=$HOME/Lethean",
    "chain": "$HOME/.deno/bin/deno run --allow-net --allow-env --allow-run --allow-read --allow-write --unstable src/server.ts daemon chain start",
    "vpn": "$HOME/.deno/bin/deno run --allow-net --allow-env --allow-run --allow-read --allow-write --unstable src/server.ts vpn provider  --help",
    "server:compile": "deno compile --unstable --allow-env --allow-run --allow-net --allow-read --allow-write --output ../build/cli/lthn --target  ",
    "bundle": "deno bundle --unstable src/lethean-cli.ts ../build/lthn.js",
    "server:build:linux": "npm run server:compile -- x86_64-unknown-linux-gnu src/server.ts",
    "server:build:macos-intel": "npm run server:compile -- x86_64-apple-darwin src/server.ts",
    "server:build:macos-arm": "npm run server:compile -- aarch64-apple-darwin src/server.ts",
    "release:windows": "npm run server:compile -- x86_64-pc-windows-msvc src/server.ts",
    "ci:windows": "%HOME%\\.deno\\bin\\deno compile --unstable --allow-env --allow-run --allow-net --allow-read --allow-write --output ../build/cli/lthn --target x86_64-pc-windows-msvc src/server.ts",
    "lint": "eslint 'src/**/*.{ts,html,scss}' --cache",
    "lint:fix": "eslint 'src/**/*.{ts,html,scss}' --cache --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/letheanVPN/dvpn.git"
  },
  "author": "",
  "license": "GPL-3",
  "bugs": {
    "url": "https://gitlab.com/lthn/projects/vpn/node/issues"
  },
  "homepage": "https://gitlab.com/lthn/projects/vpn/node#readme",
  "dependencies": {
    "ansi_up": "^5.1.0",
    "ansi-to-html": "^0.7.2",
    "ansicolor": "^1.1.95",
    "i": "^0.3.7",
    "npm": "^8.1.2",
    "openpgp": "^5.0.0",
    "pem-ts": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1"
  }
}
